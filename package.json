{
  "name": "mesmer-xml-gui",
  "description": "MESMER XML GUI.",
  "source": "src/index.html",
  "version": "0.1.0",
  "scripts": {
    "start": "parcel",
    "build": "parcel build"
  },
  "targets": {
    "main": {
      "context": "browser",
      "includeNodeModules": true,
      "sourceMap": true,
      "engines": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Firefox versions",
          "last 2 Safari versions",
          "last 2 Edge versions"
        ]
      },
      "distDir": "./dist",
      "publicUrl": "/mxg-pwa/dist/main/"
    }
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.11.0",
    "@parcel/transformer-webmanifest": "^2.11.0",
    "parcel": "^2.11.0",
    "typescript": "^5.3.3"
  },
  "author": "Andy Turner"
}