{
  "name": "mxg-pwa",
  "description": "MESMER XML GUI PWA.",
  "source": "src/index.html",
  "version": "0.9",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "build": "parcel build",
    "start": "npx parcel --dist-dir dist/main src/index.html -p 3460"
  },
  "targets": {
    "main": {
      "context": "browser",
      "includeNodeModules": true,
      "sourceMap": true,
      "engines": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Firefox versions",
          "last 2 Safari versions",
          "last 2 Edge versions"
        ]
      },
      "distDir": "dist/main",
      "publicUrl": "/mxg-pwa/dist/main"
    },
    "0.9": {
      "context": "browser",
      "includeNodeModules": true,
      "sourceMap": true,
      "engines": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Firefox versions",
          "last 2 Safari versions",
          "last 2 Edge versions"
        ]
      },
      "distDir": "dist/0.9",
      "publicUrl": "/mxg-pwa/dist/0.9"
    }
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.11.0",
    "@parcel/transformer-webmanifest": "^2.11.0",
    "parcel": "^2.11.0",
    "typescript": "^5.3.3"
  },
  "author": "Andy Turner"
}
